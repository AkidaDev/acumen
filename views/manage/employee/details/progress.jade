.box.box-primary
  .box-header
    h3.box-title Working On
  // /.box-header
  .box-body.table-responsive.no-padding
    table.table.table-hover
      tr
        th(style='width: 10px') #
        th Task
        th Progress
        th(style='width: 40px') Label
      tr
        td 1.
        td Update software
        td
          .progress.progress-xs
            .progress-bar.progress-bar-danger.progress-striped(style='width: 55%')
        td
          span.badge.bg-red 55%
      tr
        td 2.
        td Clean database
        td
          .progress.progress-xs
            .progress-bar.progress-bar-yellow.progress-striped(style='width: 70%')
        td
          span.badge.bg-yellow 70%
      tr
        td 3.
        td Cron job running
        td
          .progress.progress-xs.progress-striped.active
            .progress-bar.progress-bar-primary(style='width: 30%')
        td
          span.badge.bg-light-blue 30%
      tr
        td 4.
        td Fix and squish bugs
        td
          .progress.progress-xs.progress-striped.active
            .progress-bar.progress-bar-success(style='width: 90%')
        td
          span.badge.bg-green 90%
  // /.box-body
// /.box
